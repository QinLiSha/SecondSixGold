<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_register"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_blue_lightblue"
    android:orientation="vertical"
    tools:context="com.lisa.administrator.sixgold.activity.RegisterActivity">

    <com.lisa.administrator.sixgold.view.ActionBarTwoImg
        android:id="@+id/actionbarView_twoImg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <!--<View-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="@dimen/px_100" />-->

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/xinxinblue"
        android:layout_margin="@dimen/px_40"
        android:layout_gravity="center"
        android:id="@+id/imageView4" />

    <EditText
        android:id="@+id/editText6"
        style="@style/my_edittext_style_telephone_num" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_20" />

    <LinearLayout
        android:layout_width="@dimen/button_normal_width"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/px_20"
        android:orientation="horizontal"
        android:weightSum="21">

        <EditText
            android:id="@+id/editText14"
            style="@style/my_edittext_style_normal"
            android:layout_width="0dp"
            android:layout_margin="@dimen/px_0"
            android:layout_weight="10"
            android:hint="@string/please_enter_verification_code" />

        <TextView
            android:layout_width="@dimen/px_0"
            android:layout_height="wrap_content"
            android:layout_weight="1" />

        <Button
            android:id="@+id/button2"
            style="@style/my_button_style_01"
            android:layout_width="0dp"
            android:layout_margin="@dimen/px_0"
            android:layout_weight="10"
            android:text="@string/ask_for_verification_code" />

    </LinearLayout>

    <EditText
        android:id="@+id/editText5"
        style="@style/my_edittext_style_normal"
        android:hint="@string/please_enter_invitation_code" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_20" />

    <EditText
        android:id="@+id/editText12"
        style="@style/my_edittext_style_password" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_20" />

    <EditText
        android:id="@+id/editText13"
        style="@style/my_edittext_style_password" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_20" />

    <Button
        android:id="@+id/button3"
        style="@style/my_button_style_01"
        android:text="下一步" />
</LinearLayout>


