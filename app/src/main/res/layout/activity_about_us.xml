<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_about_us"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.lisa.administrator.sixgold.activity.mine.AboutUsActivity">

    <com.lisa.administrator.sixgold.view.ActionBarTwoImg
        android:id="@+id/actionbarView_twoImg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        style="@style/my_textview_style_normal_default"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/px_30"
        android:background="@color/white"
        android:padding="@dimen/px_30"
        android:text="@string/about_us_details"
        android:textSize="@dimen/textsize_big" />
</LinearLayout>
